<%= simple_form_for([:admin, @user]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :ffff, label: "FFFF" %>
    <%= f.input :admin %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= cancel_link([:admin, @user]) %>
    <%= destroy_button([:admin, @user]) %>
  </div>
<% end %>
