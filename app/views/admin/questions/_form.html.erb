<%= simple_form_for([:admin, @question]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :question %>
    <%= f.input :acknowledged %>
    <%= f.input :done %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
